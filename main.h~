#ifndef main_h
#define main_h

#include <stdlib.h>
#include <unistd.h>
#include <stdarg.h>
#include <string.h>
/**
 *_putchar - a fonction that prints a character
 *@c: the character to print
 *Return: an int
 */
/**
 *_printf - a function that print things
 *@format: a pointer to char
 *Return: an int the number of characters printed
 */

int _printf(const char *format, ...);
int _putchar(char c);

int _putchar(char c)
{
	return (write(1, &c, 1));
}

#endif
